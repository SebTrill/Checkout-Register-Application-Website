<UserControl x:Class="FriedPiper.PointOfSale.NotFinishedDisplay"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:FriedPiper.PointOfSale"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition/>
            <RowDefinition/>
            <RowDefinition/>
            <RowDefinition/>
            <RowDefinition/>
            <RowDefinition/>
        </Grid.RowDefinitions>
        <TextBlock Name="joe" Grid.RowSpan="5" VerticalAlignment="Center" HorizontalAlignment="Center" Text="NOT FINISHED" TextWrapping="Wrap" FontSize="100"/>
        <Button Name="jeff" Click="BackHandler" Grid.Row="6" VerticalAlignment="Bottom" HorizontalAlignment="Right" Height="60" Width="200" FontSize="35">Go Back</Button>
    </Grid>
</UserControl>
